dss:dss_Dataset
a dcat:Dataset ;
a schema:Dataset ;
        dc:title "DSS Schema " ;
		dc:description "Dutch Ships and Sailors common terms" ;
		schema:distribution [
			a schema:DataDownload ; 
			schema:schema:encodingFormat "application/sparql-query" ; 
			schema:contentURL "http://semanticweb.cs.vu.nl/dss/sparql" ];
		schema:distribution [
			a schema:DataDownload ; 
			dc:title "named graphs for DSS common terms" ;
			schema:encodingFormat "text/turtle" ;
            schema:contentUrl dss:dss_schema.ttl,
				dss:dss_images.ttl,
				<http://purl.org/collections/nl/dss/alignments/al_all_place_2_geonames.ttl>,
				<http://purl.org/collections/nl/dss/alignments/dbpedia_links.ttl>,
				dss:dss_provenance.ttl
			].
